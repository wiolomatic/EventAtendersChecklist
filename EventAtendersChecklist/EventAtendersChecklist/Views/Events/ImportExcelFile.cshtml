@{
    ViewBag.Title = "ReadExcelUsingEpplus";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Read Excel</h2>

@using (Html.BeginForm("ReadExcel", "Events", FormMethod.Post, new { enctype = "multipart/form-data"}))
{
    <div class="form-group">
        <input type="file" id="dataFile" name="upload"   />
    </div>

    <div class="form-group">
        <input type="submit" name="btnupload" value="Upload" class="btn btn-success" style = "width:120px; height:35px" />
        <label>@ViewBag.Message</label>
    </div>
}
